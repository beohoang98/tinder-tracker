{
    "version": "0.0.1",
    "manifest_version": 2,
    "name": "Tinder Tracker",
    "background": {
        "scripts": ["dist/background.js"],
        "persistent": true
    },
    "permissions": [
        "webRequest",
        "storage",
        "http://tinder.com/*",
        "https://tinder.com/*",
        "*://api.gotinder.com/*"
    ]
}
